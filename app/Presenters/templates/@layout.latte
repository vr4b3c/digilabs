    <body> 

        
        <header>
            <div class="container">
                    Chuck rocks!
            </div>
        </header>
        <nav>
            <div class="container">
                <ul>
                    <li>
                        <a n:href=":GetRandomJoke!" class="ajax">Random joke!</a>
                    </li>
                    <li>
                        <button>Only from authors with same initials!</button>
                    </li>  
                    <li>
                        <button>Only valid ones</button>
                    </li>   
                    <li>
                        <button>+/- 1 month</button>
                    </li>      
                    <li>
                        <button>Valid calculation</button>
                    </li>          
                </ul>          
            </div> 
        </nav>
        
        {include content}    
        
        {snippet randomJoke}
            {include chuck/randomJoke.latte}
        {/snippet}
         



    </body>
